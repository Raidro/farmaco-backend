generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model HumanMedicine {

  id String @id @default(uuid())
  name String
  valid DateTime
  qnt_caixa Int
  qnt_comprimidos Int 

  @@map("HumanMedicines")


}

model AnimalMedicine {

id String @id @default(uuid())
  name String
  valid DateTime
  qnt_caixa Int
  qnt_comprimidos Int 

  @@map("AnimalMedicines")

}